{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true,
    "jest": true
  },
  "rules": {
    "require-jsdoc": "off",
    "operator-linebreak": "off"
  },
  "plugins": ["jest"],
  "extends": ["eslint:recommended", "google", "prettier"]
}
